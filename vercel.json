{
  "version": 2,
  "name": "bahae-kenikssi-portfolio",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "framework": "vite",
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 10
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "github": {
    "silent": true
  }
}